FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -2 0 0 0 0];

internalField   uniform 0.0084;

boundaryField
{
    wall
    {
        type            kqRWallFunction;
        value           uniform 0.0084;
    }
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           uniform 0.0084;
    }
    outOrIn
    {
        type            inletOutlet;
        inletValue      uniform 0.0084;
        value           uniform 0.0084;
    }
    frontAndBack
    {
        type            empty;
    }
}


