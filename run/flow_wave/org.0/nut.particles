FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -1 0 0 0 0];

internalField       uniform 0;


boundaryField
{
    wall
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    1.8e-4;
        value           uniform 0.3864;
    }
    outOrIn
    {
        type            inletOutlet;
        inletValue      uniform 0.3864;
        value           uniform 0.3864;
    }
    frontAndBack
    {
        type            empty;
    }
}


// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
