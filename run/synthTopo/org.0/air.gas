FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      air.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.999;

boundaryField
{
    atm
    {
        type            inletOutlet;
        inletValue      uniform 0.999;
        value           uniform 0.999;
    }

    top
    {
        type            inletOutlet;
        inletValue      uniform 0.999;
        value           uniform 0.999;
    }

    terrain
    {
        type            zeroGradient;
    }
    crater
    {
        type            zeroGradient;
    }
}


