FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            calculated;
        value           $internalField;
    }

    atm
    {
        type            calculated;
        value           $internalField;
    }

    terrain
    {
        type            calculated;
        value           $internalField;
    }

    crater
    {
        type            calculated;
        value           $internalField;
    }
}

